    <form action="/narasimha" method="post">
      <div class="card form bg-light mb-3" style="max-width: 58rem;">
        <div class="container">
          <div class="form-floating mb-3">
            <label for="floatingInput">Destination Name</label>
            <input type="text" class="form-control" id="floatingInput" name="Dest_name" value="Narasimha Parvatha">
          </div>
  
          <div class="form-floating mb-3">
            <label for="floatingInput">Trek Distance</label>
            <input type="text" class="form-control" name="Trek_Distance" value="10KM" >
          </div>
          <br>
          <hr>
          <br>
          <div class="form-floating">
            <label for="floatingPassword">Pickup Point</label>
           
            <select class="form-select form-select-lg" name="Pickup_point" aria-label=".form-select-lg example">
              <option selected>( select )</option>
              <option value="Bangalore University">Bangalore University</option>
              <option value="JP Nagar">JP Nagar</option>
              <option value="Mysore Road">Mysore Road</option>
            </select>
          </div>
          <br>
          <div class="form-floating mb-3">
            <label for="floatingInput">Number of People</label>
            <input type="number" min="1" max="10" class="form-control" id="floatingInput" name="No_of_Members"  placeholder="1-10">
          </div>
          
          <div class="form-floating">
            <label for="floatingPassword">Date</label>
            <input type="text" class="form-control" id="floatingInput" name="Date" placeholder="yyyy-mm-dd">
            
          </div>
          <br>
          <hr>
          <br>
          <div class="form-floating">
            <label for="floatingPassword">Mode of payment</label>
            <br>
            <select class="form-select form-select-lg" name="Mode_of_payment" aria-label=".form-select-lg example">
              <option selected>( select )</option>
              <option value="UPI">UPI</option>
              <option value="Credit Card">Credit Card</option>
              <option value="Net Banking">Net Banking</option>
            </select>
          </div>
          <br>
          <div class="form-floating">
            <label for="floatingPassword">Coupons</label>
            <br>
            <select class="form-select form-select-lg" name="Coupons" aria-label=".form-select-lg example">
              <option selected>( select )</option>
              <option value="0.1">10%</option>
              <option value="0.15">15%</option>
              <option value="0.2">20%</option>
            </select>
          </div>
          <br>
          <div class="form-floating mb-3">
            <label for="floatingInput">Cost</label>
            <input type="text" class="form-control" id="floatingInput" name="Cost" value="2200" >
          </div>
          <br>
  
  
          <button type="submit" class="btn btn-primary btn-block" value="submit">Confirm Booking</button>
        </div>
      </div>
    </form>









